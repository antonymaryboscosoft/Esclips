package javamukesh;
//clss 12 
public class Callbyvalueandreference {
	
	public static void main(String[] args) {
//		Callbyvalueandreference obj = new  Callbyvalueandreference();
		int a = 10;
		int b = 20 ;
		test(a, b);
		System.out.println('z');
	}
public static int test(int x , int y){
	int z = x+y ;
	return z ;
}
	
}
