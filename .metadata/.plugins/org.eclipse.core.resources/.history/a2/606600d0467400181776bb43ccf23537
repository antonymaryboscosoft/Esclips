package javamukesh;

public class Stringconcordination {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
int a = 10;
int b = 20 ;
String x = "hellow world";
String y = "Java";
System.out.println(a+b);
System.out.println(x+y+a+b);
System.out.println(x+y+(a+b));
//	class 3 naveen
	int i=1;
	while(i<=10){
		System.out.print(i);
		i=i+1;
	}
}

}
